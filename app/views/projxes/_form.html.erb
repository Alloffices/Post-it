<div class="project-p">
<div class="container">
    <div class="logy">
      <%= link_to "NEW PROJECT", current_user %>
    </div>

<div class="container">
<%= form_for(@projx) do |f| %>
  <% if @projx.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@projx.errors.count, "error") %> prohibited this projx from being saved:</h2>

      <ul>
      <% @projx.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
<br />
  <div class="message-form">
    <h6>
      Type of project
    </h6>
  </div>
  <%= f.select :type_skill, ["Branding", "Fashion Design", "Fashion Show", "Graphic Design", "Modeling", "Photography", "Post-production", "Videography", "Wedding", "Writing", "Other"], prompt: "Choose", class: "form-control" %>
  </p>

<div class="field">
    <div class="message-form">
      <h6>
        Project location
      </h6>
    </div>
      <%= f.select :location,[
      "Alabama, AL",
      "Alaska, AK",
      "Arizona', AZ",
      "Arkansas, AR",
      "California, CA",
      "Colorado, CO",
      "Connecticut', CT",
      "Delaware, DE",
      "District of Columbia, DC",
      "Florida, FL",
      "Georgia, GA",
      "Hawaii, HI",
      "Idaho, ID",
      "Illinois, IL",
      "Indiana, IN",
      "Iowa, IA",
      "Kansas, KS",
      "Kentucky, KY",
      "Louisiana, LA",
      "Maine, ME",
      "Maryland, MD",
      "Massachusetts, MA",
      "Michigan, MI",
      "Minnesota, MN",
      "Mississippi, MS",
      "Missouri, MO",
      "Montana, MT",
      "Nebraska, NE",
      "Nevada, NV",
      "New Hampshire, NH",
      "New Jersey, NJ",
      "New Mexico, NM",
      "New York, NY",
      "North Carolina, NC",
      "North Dakota, ND",
      "Ohio, OH",
      "Oklahoma, OK",
      "Oregon', 'OR",
      "Pennsylvania, PA",
      "Puerto Rico, PR",
      "Rhode Island, RI",
      "South Carolina, SC",
      "South Dakota, SD",
      "Tennessee, TN",
      "Texas, TX",
      "Utah, UT",
      "Vermont, VT",
      "Virginia, VA",
      "Washington, WA",
      "West Virginia, WV",
      "Wisconsin', WI",
      "Wyoming, WY"], prompt: "Choose", class: "form-control" %>
    </div>

  <div class="field">
    <div class="message-form">
      <h6>
        Title
      </h6>
    </div>
    <%= f.text_field :title, class: "form-control", maxlength: 88 %>
  </div>
  <div class="field">
    <div class="message-form">
      <h6>
        What's your project about?
      </h6>
    </div>
    <%= f.text_area :description, class: "form-control" %>
  </div>

  <div class="field">
    <div class="message-form">
      <h6>
        Your Website
      </h6>
    </div>
    <%= f.text_field :website, class: "form-control" %>
  </div>

  <div class="field">
    <div class="message-form">
      <h6>
        Project Date
      </h6>
    </div>
    <%= f.date_field :date, class: "form-control" %>
  </div>
  <hr />
  <div>
    <%= f.button :submit, class: "btn btn-primary btn-block" %>
  </div>
<% end %>
</div>
</div>
</div>

